<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        tools:context=".MainActivity" android:id="@+id/linearLayout" android:orientation="horizontal">
    <TextView
            android:layout_width="175dp"
            android:layout_height="100dp"
            android:text="@string/redes_disponibles"
            android:textSize="30sp"
            android:textAlignment="center"
            android:id="@+id/textView" android:layout_marginTop="20dp" android:layout_marginBottom="21dp"
    />


    <ListView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/myListView"


            android:baselineAligned="false"/>


</LinearLayout>